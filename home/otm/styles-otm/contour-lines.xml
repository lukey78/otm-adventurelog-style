<Style name="contour-lines">
	<!-- 100 m -->
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom11;
		<Filter>([height] % 100 = 0) and ([height] != 0)</Filter>
		<LineSymbolizer stroke="#999999" stroke-width="0.2" smooth="0.5" />
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom12;
		<Filter>([height] % 100 = 0) and ([height] != 0)</Filter>
		<LineSymbolizer stroke="#999999" stroke-width="0.3" smooth="0.5" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
		<Filter>([height] % 100 = 0) and ([height] != 0)</Filter>
		<LineSymbolizer stroke="#999999" stroke-width="0.4" smooth="0.5" />
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([height] % 100 = 0) and ([height] != 0)</Filter>
		<LineSymbolizer stroke="#999999" stroke-width="0.5" smooth="0.5" />
	</Rule>

	<!-- 20 m -->
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom13;
		<Filter>([height] % 100 != 0) and ([height] % 20 = 0) and ([height] != 0)</Filter>
		<LineSymbolizer stroke="#999999" stroke-width="0.2" smooth="0.8" />
	</Rule>

	<!-- 10 m only for higher zoom levels -->
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([height] % 100 != 0) and ([height] % 10 = 0) and ([height] != 0)</Filter>
		<LineSymbolizer stroke="#999999" stroke-width="0.2" smooth="0.8" />
	</Rule>

</Style>
