<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs +over" buffer-size="256">

    <Style name="slopes">
        <Rule>
            <RasterSymbolizer scaling="bicubic" />
        </Rule>
    </Style>
    <Layer name="slopes">
        <StyleName>slopes</StyleName>
        <Datasource>
            <Parameter name="type">gdal</Parameter>
            <Parameter name="file">/mnt/data/srtm/warp-30-slope-map.tif</Parameter>
            <Parameter name="format">tiff</Parameter>
        </Datasource>
    </Layer>
</Map>
